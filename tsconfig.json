{
    "compilerOptions": {
      "outDir": "./dist/",
      "sourceMap": true,
      "noImplicitAny": true,
      "module": "es6",
      "target": "es6",
      "jsx": "react",
      "allowJs": true,
      "baseUrl": "src",
      "paths": {
          "src/*": ["*"]
      },
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "allowSyntheticDefaultImports": true
    }
}